    title MISOCIAL
    body
        include helpers/vars.pug
        include helpers/mixins.pug

        .header
            .wrapper
                .header_line
                    a.header_line__title
                        img.header_line__logo(src="img/logo.png" alt="")
                        h2.header_line__text MISOCIAL
                    .header_line__subtitle
                        a.header_line__link(href="#") Home
                        a.header_line__link(href="#")  Why MISOCIAL
                        a.header_line__link(href="#") Features
                        a.header_line__link(href="#") Articles
                        a.header_line__link(href="#") Pricing
                        a.header_line__link(href="#" class="button") GET STARTED
            .header_theme
                .header_theme__text
                    p.header_theme__title SOCIAL MARKETING & ANALYTICS
                    p.header_theme__logo Pricing Table
                    p.header_theme__sub-title Lorem ipsum dolor sit amet, consectetur adipisicing <br> elit, sed do eiusmod tempor incididunt ut.

            .header_sub_arrow
                img(src="img/arrow.png" alt="")
            include components/header.pug

        main
            .main
                .wrapper 

                    .main_button_plan
                        p.main_button_plan_title MISOCIAL
                        p.main_button_plan_logo Select a Plan
                        p.main_button_plan_sub-title Lorem ipsum dolor sit amet, consectetur adipisicing <br> elit, sed do eiusmod tempor incididunt ut.   

                    .main_theme
                        +card("dollars0", "img/free-plan.png", "20") 
                        +card("dollars1", "img/dollars-plan1.png", "29")
                        +card("dollars2", "img/dollars-plan2.png", "49")  
                .main_text-maps
                    .wrapper  
                        div
                            .main_text-maps_item
                                p.main__text-maps-title SOCAL MARKETING & ANALYTICS
                                p.main__text-maps-subtitle Take control of your data
                        div 
                            .main_text_maps_link 
                                a.main_text_maps_button(href="#") get started
                                a.main_text_maps_button(href="#") explore features
.wrapper 

